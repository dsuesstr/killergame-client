{
  "name": "Killergame",
  "version": "0.0.1",
  "description": "It's a killergame",
  "main": "main.js",
  "dependencies": {
    "tsd": "*",
    "glob": "*",
    "gulp": "*",
    "gulp-concat": "*",
    "gulp-uglify": "*",
    "gulp-if": "*",
    "gulp-notify": "*",
    "gulp-minify-css": "*",
    "gulp-clean": "*",
    "gulp-size": "*",
